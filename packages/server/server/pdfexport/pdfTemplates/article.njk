                </div>
                <div class="journal">
                    {# The name of the journal. Would be great to get it from kotahi config #}
                    <p class="journal-name">{{ article.config.publishing.journalName }}</p>
                </div>
            </aside>
            <header>
               
                {# the title of the article. #}
                <h1>{{ article.submission.$title | safe }}</h1>
                {# the author list is a custom build #}
                <section class="authors-and-affiliation">
                    {{ article.articleMetadata.formattedAuthors | safe }}
                </section>
                <aside class="doi-licence">
                    <div class="meta-text">
                        {# // this is reused for citation in the footer #}
                        <p class="forcitation"
                           id="cloneCite"
                           data-id="cloneCite"
                           style="display: none">
                            <span class="journaltitle">{{article.config.publishing.journalName}}</span>
                            <span class="citation">{{ article.submission.datePublished }}<span></span></span>
                        </p>
                        <div class="dates">
                            {% if article.submission.dateReceived %}
                                <p>
                                    <span>Received:</span>
                                    <time>{{ article.submission.dateReceived }}</time>
                                </p>
                            {% endif %}
                            {% if article.submission.dateAccepted %}
                                <p>
                                    <span>Accepted:</span>
                                    <time>{{ article.submission.dateAccepted }}</time>
                                </p>
                            {% endif %}
                            {% if article.submission.datePublished %}
                                <p>
                                    <span>Published:</span>
                                    <time>{{ article.submission.datePublished }}</time>
                                </p>
                            {% endif %}
                        </div>
                    </div>
                    {# the licence is cc-by. You can update here here:  #}
                    {% if article.config.publishing.licenseUrl %}
                    <p class="licence">
                        This article is published under 
                        <a href="{{article.config.publishing.licenseUrl}}">{{article.config.publishing.licenseUrl}}</a>.
                    </p>
                    {% endif %}
                    <div class="logos">
                        <a class="doi" href="{{ article.submission.$doi }}"><span>{{ article.submission.$doi }}</span></a>
                    </div>
                    <!-- </div> -->
                </aside>
            </header>
            <article>
                <div id="abstract-wrapper">
                    <h2>Abstract</h2>
                    {{ article.submission.$abstract | safe }}
                    {% if article.articleMetadata.keywords %}
                        <p class="keywords">
                            <span class="title">Keywords:</span>{{ article.articleMetadata.keywords }}
                        </p>
                    {% endif %}
                </div>
                <section class="article">
                    {{ article.meta.source | safe }}
                </section>
            </article>
        </main>
    </body>
