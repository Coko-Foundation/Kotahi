{
  "name": "kotahi",
  "version": "1.8.1",
  "private": true,
  "description": "Kotahi - open journals",
  "repository": {
    "type": "git",
    "url": "https://gitlab.coko.foundation/kotahi/kotahi"
  },
  "license": "MIT",
  "scripts": {
    "__cleanNodeModules": "find . -name 'node_modules' -type d -prune -print -exec rm -rf '{}' \\;",
    "build": "NODE_ENV=production pubsweet build",
    "client": "pubsweet start:client",
    "console": "node --experimental-repl-await -r ./scripts/preload.js",
    "cz": "git-cz",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:js": "eslint .",
    "lint:style": "stylelint **.js",
    "precommit": "lint-staged",
    "reset": "pubsweet setupdb --clobber",
    "seedFromDump": "node scripts/seedFromDump.js",
    "server": "yarn dotenv yarn pubsweet start:server",
    "server:test": "yarn dotenv node node_modules/pubsweet-server/src/start",
    "start:server-and-client": "start-test server 'http://localhost:3000/healthcheck' client",
    "start:services": "docker-compose up db",
    "storybook": "coko-storybook",
    "test:server-and-client": "start-test server:test 'http://localhost:3000/healthcheck' client",
    "test:all:chrome": "start-test test:server-and-client 4000 test:chrome:aperture",
    "test:all:firefox": "start-test test:server-and-client 4000 test:firefox:aperture",
    "test:chrome:aperture": "cypress run --browser chrome --spec 'cypress/e2e/aperture/**.js'",
    "test:firefox:aperture": "cypress run --browser firefox --headless --spec 'cypress/e2e/aperture/**.js'",
    "test:all:chrome:elife": "start-test test:server-and-client 4000 test:chrome:elife",
    "test:all:chrome:elife:samsung": "start-test test:server-and-client 4000 test:chrome:elife:samsung",
    "test:all:chrome:elife:samsung:tablet": "start-test test:server-and-client 4000 test:chrome:elife:samsung:tablet",
    "test:all:chrome:elife:iPhone": "start-test test:server-and-client 4000 test:chrome:elife:iPhone",
    "test:all:chrome:elife:iPhone:tablet": "start-test test:server-and-client 4000 test:chrome:elife:iPhone:tablet",
    "test:all:chrome:elife:hd:laptop": "start-test test:server-and-client 4000 test:chrome:elife:hd:laptop",
    "test:chrome:elife": "cypress run --browser chrome --headless --spec 'cypress/e2e/elife/**.js'",
    "test:chrome:elife:samsung": "cypress run --env userAgent=phoneSamsung --browser chrome --headless --spec 'cypress/e2e/elife/**.js'",
    "test:chrome:elife:samsung:tablet": "cypress run --env userAgent=tabletSamsung --browser chrome --headless --spec 'cypress/e2e/elife/**.js'",
    "test:chrome:elife:iPhone": "cypress run --env userAgent=phoneSafari --browser chrome --spec 'cypress/e2e/elife/**.js'",
    "test:chrome:elife:iPhone:tablet": "cypress run --env userAgent=tabletSafari --browser chrome --headless --spec 'cypress/e2e/elife/**.js'",
    "test:chrome:elife:hd:laptop": "cypress run --env userAgent=hdLaptop --browser chrome --headless --spec 'cypress/e2e/elife/**.js'",
    "test:all:firefox:elife": "start-test test:server-and-client 4000 test:firefox:elife",
    "test:all:firefox:elife:samsung": "start-test test:server-and-client 4000 test:firefox:elife:samsung",
    "test:all:firefox:elife:samsung:tablet": "start-test test:server-and-client 4000 test:firefox:elife:samsung:tablet",
    "test:all:firefox:elife:iPhone": "start-test test:server-and-client 4000 test:firefox:elife:iPhone",
    "test:all:firefox:elife:iPhone:tablet": "start-test test:server-and-client 4000 test:firefox:elife:iPhone:tablet",
    "test:all:firefox:elife:hd:laptop": "start-test test:server-and-client 4000 test:firefox:elife:hd:laptop",
    "test:firefox:elife": "cypress run --browser firefox --headless --spec 'cypress/e2e/elife/**.js'",
    "test:firefox:elife:samsung": "cypress run --env userAgent=phoneSamsung --browser firefox --headless --spec 'cypress/e2e/elife/**.js'",
    "test:firefox:elife:samsung:tablet": "cypress run --env userAgent=tabletSamsung --browser firefox --headless --spec 'cypress/e2e/elife/**.js'",
    "test:firefox:elife:iPhone": "cypress run --env userAgent=phoneSafari --browser firefox --spec 'cypress/e2e/elife/**.js'",
    "test:firefox:elife:iPhone:tablet": "cypress run --env userAgent=tabletSafari --browser firefox --headless --spec 'cypress/e2e/elife/**.js'",
    "test:firefox:elife:hd:laptop": "cypress run --env userAgent=hdLaptop --browser chrome --spec 'cypress/e2e/elife/**.js'",
